<!DOCTYPE html>
<html>
<%- include('../partials/head.ejs', {title: "Delete Room"}) %>
<body>

<%- include('../partials/headerSwitch') %>

<div class=main_join>
<div class="join_box">
<div class="jbox">
	<p>Delete a room</p>
	<div id="join_container">
    <select name="room" id="delete-input">
			<% rooms.forEach(function(room){ %>
				<option value="<%= room.room %>"><%= room.room %></option>
			<% }); %>
		</select>
	<div id="delete-btn">Delete</div>
  <p id="msg"></p>
</div>
</div>
</div>
</div>

</body>
</html>
