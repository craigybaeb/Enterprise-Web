<!DOCTYPE html>
<html>
<%- include('../../partials/head.ejs', {title: "Delete Room"}) %>
<body>

<%- include('../../partials/headerSwitch') %>

<div class=main_join>
<div class="join_box">
<div class="jbox">
	<p>Delete a room</p>
	<div id="join_container">
		<form>
    <select autofocus="on" name="room" id="delete-input">
			<% rooms.forEach(function(room){ %>
				<option value="<%= room.room %>"><%= room.room %></option>
			<% }); %>
		</select>
		<div class="admin-button-container">
			<a href="/admin"><div class="btn">Back</div><a/>
			<div id="delete-btn" class="btn admin-control-btn">Delete</div>
		</div>
	</form>
  <p id="msg"></p>
</div>
</div>
</div>
</div>

</body>
</html>
