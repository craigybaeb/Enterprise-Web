<% switch (privilege) {
case "1" : %>
      <%- include('../partials/header', {title: "Join Room", links: [{"/" : "Join Room"},
                                                                     {"/logout" : "Logout"}]}) %>
      <% break;

case "2" : %>
<%- include('../partials/header', {title: "Join Room", links: [{"/" : "Join Room"},
                                                            {"/room/add" : "Add Room"},
                                                            {"/room/delete" : "Delete Room"},
                                                            {"/logout" : "Logout"}]}) %>
      <% break;

case "3" : %>
<%- include('../partials/header', {title: "Join Room", links: [{"/" : "Join Room"},
                                                            {"/room/add" : "Add Room"},
                                                            {"/room/delete" : "Delete Room"},
                                                            {"/privilege" : "Edit User Privileges"},
                                                            {"/logout" : "Logout"}]}) %>
      <% break;
default : %>
<%- include('../partials/header', {title: "Join Room", links: [{"/register" : "Register"},
                                                               {"/login" : "Login"}]}) %>
      <% break;} %>
