<%# Checks the user level and displays the correct header accordingly %>
<% switch (privilege) {
  // User is not admin
  case "1" : %>
        <%- include('../partials/header', {links: [{"/" : "Join Room"},
                                                   {"/logout" : "Logout"}]}) %>
        <% break;

  // User is admin
  case "2" :
  case "3" : %>
        <%- include('../partials/header', {links: [{"/" : "Join Room"},
                                                              {"/admin" : "Admin Portal"},
                                                              {"/logout" : "Logout"}]}) %>
        <% break;
  // Default to non-admin for security
  default : %>
        <%- include('../partials/header', {links: [{"/login" : "Login"},
                                                    {"/register" : "Register"}]}) %>
      <% break;} %>
